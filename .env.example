# Google AI API Key (required for Gemini)
GOOGLE_GENERATIVE_AI_API_KEY=your_api_key_here

# Optional: Port for local development
PORT=8000

# Cache Configuration (optional)
# Enable or disable caching of generated content (default: true)
CACHE_ENABLED=true
# Cache TTL (Time To Live) in seconds (default: 3600 = 1 hour)
CACHE_TTL=3600
